@article{bisio2014,
  title = {Pushing the {{High-Energy Limit}} of {{Plasmonics}}},
  author = {Bisio, Francesco and Proietti Zaccaria, Remo and Moroni, Riccardo and Maidecchi, Giulia and Alabastri, Alessandro and Gonella, Grazia and Giglia, Angelo and Andolfi, Laura and Nannarone, Stefano and Mattera, Lorenzo and Canepa, Maurizio},
