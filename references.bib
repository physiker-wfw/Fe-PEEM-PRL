@article{gohler2021,
  title = {Monolayer Attenuation Length of Low-Energy Electrons in {{Gd}} and {{Tb}}},
  author = {G{\"o}hler, Karen and Schmidt, Anke B. and Donath, Markus},
  year = {2021},
  month = mar,
  journal = {Journal of Vacuum Science \& Technology A},
  volume = {39},
  number = {2},
  pages = {023205},
  issn = {0734-2101, 1520-8559},
  doi = {10.1116/6.0000754},
  abstract = {The electron attenuation length is a material property, which defines the probing depth of all electron spectroscopies. For kinetic energies below 50 eV, very few data exist in the literature, although this energy range is vital for electron spectroscopy using low-energy electrons, e.g., for studying the valence electronic band structure. Extremely short attenuation lengths are expected for the transition metals gadolinium and terbium. To determine the electron attenuation length in these materials, we conducted inverse-photoemission overlayer experiments with ultrathin Gd and Tb films on W(110). For 10-eV electrons, we find an attenuation length of about one monolayer in Gd and even less in Tb, i.e., two thirds (or more) of the electrons are stopped by only one monolayer. These results imply extremely short information depths for electron spectroscopies with low energies on the lanthanides Gd and Tb.},
  langid = {english},
  keywords = {Dissertationsbibliothek,main,mean free path},
  file = {D\:\\Data\\data+analysis\\Zotero Verzeichnis\\storage\\SNPRWHPC\\Göhler et al. - 2021 - Monolayer attenuation length of low-energy electro.pdf}
}

@article{gorchon2016,
  title = {Model for Multishot All-Thermal All-Optical Switching in Ferromagnets},
  author = {Gorchon, J. and Yang, Y. and Bokor, J.},
  year = {2016},
  month = jul,
  journal = {Phys. Rev. B},
  volume = {94},
  number = {2},
  pages = {020409},
  doi = {10.1103/PhysRevB.94.020409},
  abstract = {All-optical magnetic switching (AOS) is a recently observed rich and puzzling phenomenon that offers promising technological applications. However, a fundamental understanding of the underlying mechanisms remains elusive. Here we present a model for multishot helicity-dependent AOS in ferromagnetic materials based on a purely heat-driven mechanism in the presence of magnetic circular dichroism (MCD). We predict that AOS should be possible with as little as 0.5\% of MCD, after a minimum number of laser shots heat the sample close to the Curie temperature. Finally, we qualitatively reproduce the all-optically switched domain patterns observed experimentally by numerically simulating the result of multiple laser shots on an FePtC granular ferromagnetic film.},
  keywords = {C,Dissertationsbibliothek,Fe,main,multishot,Pt,switching},
  file = {D\:\\Data\\data+analysis\\Zotero Verzeichnis\\storage\\HYN3GYFT\\Gorchon2016.pdf;D\:\\Data\\data+analysis\\Zotero Verzeichnis\\storage\\MGDIBA3T\\PhysRevB.94.html}
}

@article{gorchon2016a,
  title = {Role of Electron and Phonon Temperatures in the Helicity-Independent All-Optical Switching of {{GdFeCo}}},
  author = {Gorchon, J. and Wilson, R. B. and Yang, Y. and Pattabi, A. and Chen, J. Y. and He, L. and Wang, J. P. and Li, M. and Bokor, J.},
  year = {2016},
  month = nov,
  journal = {Phys. Rev. B},
  volume = {94},
  number = {18},
  pages = {184406},
  doi = {10.1103/PhysRevB.94.184406},
  abstract = {Ultrafast optical heating of the electrons in ferrimagnetic metals can result in all-optical switching (AOS) of the magnetization. Here we report quantitative measurements of the temperature rise of GdFeCo thin films during helicity-independent AOS. Critical switching fluences are obtained as a function of the initial temperature of the sample and for laser pulse durations from 55 fs to 15 ps. We conclude that nonequilibrium phenomena are necessary for helicity-independent AOS, although the peak electron temperature does not play a critical role. Pump-probe time-resolved experiments show that the switching time increases as the pulse duration increases, with 10 ps pulses resulting in switching times of {$\sim$}13 ps. These results raise new questions about the fundamental mechanism of helicity-independent AOS.},
  keywords = {Dissertationsbibliothek,main},
  file = {D\:\\Data\\data+analysis\\Zotero Verzeichnis\\storage\\JFTKQBDP\\Gorchon2016.pdf;D\:\\Data\\data+analysis\\Zotero Verzeichnis\\storage\\B9L79FYN\\PhysRevB.94.html}
}

