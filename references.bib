@article{alexander2019,
  title = {Electronic {{Structure-Dependent Surface Plasmon Resonance}} in {{Single Au}}\textendash{{Fe Nanoalloys}}},
  author = {Alexander, Duncan T. L. and Forrer, Daniel and Rossi, Enrico and Lidorikis, Elefterios and Agnoli, Stefano and Bernasconi, Gabriel D. and Butet, J{\'e}r{\'e}my and Martin, Olivier J. F. and Amendola, Vincenzo},
  year = {2019},
  month = aug,
  journal = {Nano Lett.},
  volume = {19},
  number = {8},
  pages = {5754--5761},
  issn = {1530-6984, 1530-6992},
  doi = {10.1021/acs.nanolett.9b02396},
  abstract = {The relationship between composition and plasmonic properties in noble metal nanoalloys is still largely unexplored. Yet, nanoalloys of noble metals, such as gold, with transition elements, such as iron, have unique properties and a number of potential applications, ranging from nanomedicine to magneto-plasmonics and plasmon-enhanced catalysis. Here, we investigate the localized surface plasmon resonance at the level of the single Au-Fe nanoparticle by applying a strategy that combines experimental measurements using near field electron energy loss spectroscopy with theoretical studies via a full wave numerical analysis and density functional theory calculations of electronic structure. We show that, as the iron fraction increases, the plasmon resonance is blue-shifted and significantly damped, as a consequence of the changes in the electronic band structure of the alloy. This allows the identification of three relevant phenomena to be considered in the design and realization of any plasmonic nanoalloy, specifically: the appearance of new states around the Fermi level; the change in the free electron density of the metal; and the blue shift of interband transitions. Overall, this study provides new opportunities for the control of the optical response in Au-Fe and other plasmonic nanoalloys, which are useful for the realization of magnetoplasmonic devices for molecular sensing, thermo-plasmonics, bioimaging, photocatalysis, and the amplification of spectroscopic signals by local field enhancement.},
  langid = {english},
  keywords = {Dissertationsbibliothek,main},
  file = {D\:\\Data\\data+analysis\\Zotero Verzeichnis\\storage\\7NER4IFT\\Alexander et al. - 2019 - Electronic Structure-Dependent Surface Plasmon Res.pdf}
}

