@article{rollinger2016,
  title = {Light {{Localization}} and {{Magneto-Optic Enhancement}} in {{Ni Antidot Arrays}}},
  author = {Rollinger, Markus and Thielen, Philip and Melander, Emil and {\"O}stman, Erik and Kapaklis, Vassilios and Obry, Bj{\"o}rn and Cinchetti, Mirko and {Garc{\'i}a-Mart{\'i}n}, Antonio and Aeschlimann, Martin and Papaioannou, Evangelos Th.},
  year = {2016},
  month = apr,
  journal = {Nano Lett.},
  volume = {16},
  number = {4},
  pages = {2432--2438},
  issn = {1530-6984, 1530-6992},
  doi = {10.1021/acs.nanolett.5b05279},
  abstract = {We reveal an explicit strategy to design the magneto-optic response of a magneto-plasmonic crystal by correlating near- and far-fields effects. We use photoemission electron microscopy to map the spatial distribution of the electric near-field on a nanopatterned magnetic surface that supports plasmon polaritons. By using different photon energies and polarization states of the incident light we reveal that the electric near-field is either concentrated in spots forming a hexagonal lattice with the same symmetry as the Ni nanopattern or in stripes oriented along the {$\Gamma-$}K direction of the lattice and perpendicular to the polarization direction. We show that the polarization-dependent near-field enhancement on the patterned surface is directly correlated to both the excitation of surface plasmon polaritons on the patterned surface as well as the enhancement of the polar magneto-optical Kerr effect. We obtain a relationship between the size of the enhanced magnetooptical behavior and the polarization and wavelength of optical excitation. The engineering of the magneto-optic response based on the plasmon-induced modification of the optical properties introduces the concept of a magneto-plasmonic meta-structure.},
  langid = {english},
  keywords = {Dissertationsbibliothek,main},
  file = {D\:\\Data\\data+analysis\\Zotero Verzeichnis\\storage\\NU82NZJX\\Rollinger et al. - 2016 - Light Localization and Magneto-Optic Enhancement i.pdf}
}

