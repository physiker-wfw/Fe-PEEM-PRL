  file = {D\:\\Data\\data+analysis\\Zotero Verzeichnis\\storage\\CB3QDVLC\\Tikuišis et al. - 2017 - OPTICAL AND MAGNETO-OPTICAL PROPERTIES OF PERMALLO.pdf}
}

