@article{ostler2012,
  title = {Ultrafast Heating as a Sufficient Stimulus for Magnetization Reversal in a Ferrimagnet},
  author = {Ostler, T. A. and Barker, J. and Evans, R. F. L. and Chantrell, R. W. and Atxitia, U. and {Chubykalo-Fesenko}, O. and El Moussaoui, S. and Le Guyader, L. and Mengotti, E. and Heyderman, L. J. and Nolting, F. and Tsukamoto, A. and Itoh, A. and Afanasiev, D. and Ivanov, B. A. and Kalashnikova, A. M. and Vahaplar, K. and Mentink, J. and Kirilyuk, A. and Rasing, Th and Kimel, A. V.},
  year = {2012},
