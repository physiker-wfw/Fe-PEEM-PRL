  author = {Tikui{\v s}is, Kristupas Kazimieras and Beran, Luk{\'a}{\v s}s and Cejpek, Petr and Uhl{\'i}{\v r}ov{\'a}, Kl{\'a}ra and Hamrle, Jaroslav and Va{\v n}atka, Marek and Urb{\'a}nek, Michal and Veis, Martin},
